<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="350.0" prefWidth="390.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.QuizMasterAddDeleteController">
    <children>
        <Label fx:id="welcomeUsernameLbl" alignment="CENTER" layoutY="14.0" prefHeight="62.0" prefWidth="398.0" text="Welcome, USERNAME">
            <font>
                <Font name="Book Antiqua" size="19.0" />
            </font>
        </Label>
        <Separator layoutX="14.0" layoutY="84.0" prefHeight="4.0" prefWidth="367.0" />
        <Button fx:id="addQuizBtn" layoutX="128.0" layoutY="133.0" mnemonicParsing="false" onMouseClicked="#addQuiz" prefHeight="47.0" prefWidth="142.0" text="Add Quiz" />
        <Button fx:id="deleteQuizBtn" layoutX="128.0" layoutY="215.0" mnemonicParsing="false" onMouseClicked="#deleteQuiz" prefHeight="47.0" prefWidth="142.0" text="Delete Quiz" />
        <Label fx:id="logoutLbl" alignment="CENTER" layoutX="175.0" layoutY="285.0" onMouseClicked="#logout" prefHeight="17.0" prefWidth="48.0" text="Logout" textAlignment="CENTER" textFill="#6164f2" />
    </children>
</AnchorPane>
